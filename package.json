{
  "name": "pe-analyser",
  "version": "1.0.0",
  "description": "PE Analyser application with Node.js backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend-js && npm run dev",
    "dev:frontend": "cd frontend && PORT=3001 npm start",
    "install:all": "npm install && cd backend-js && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend-js && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["pe-analyser", "pharmacy", "analytics"],
  "author": "",
  "license": "ISC"
} 